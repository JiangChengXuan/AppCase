<configuration>
    <system.web>
      <compilation debug="true" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" executionTimeout="500"    maxRequestLength="40960"  />
    </system.web>
  <!--
  executionTimeout：请求超时时间，单位秒，默认为90秒
  maxRequestLength：设置客户端上传文件大小的限制，单位KB，默认为4096
  -->


</configuration>
